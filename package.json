{
  "name": "invbeertory",
  "description": "Simple beer inventory display",
  "version": "0.1.0",
  "author": "Cian Butler <butlerx@notthe.cloud>",
  "menu": [
    {
      "name": "Current Stock",
      "link": "/stock"
    },
    {
      "link": "/history/",
      "name": "History"
    },
    {
      "link": "/graphs/",
      "name": "Graphs"
    }
  ],
  "project": {},
  "dependencies": {
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^2.4.2",
    "chart.xkcd": "^1.1.13",
    "dotenv": "^8.2.0",
    "elasticlunr": "^0.9.5",
    "gatsby": "^2.32.3",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-offline": "^3.10.0",
    "gatsby-plugin-preact": "^4.7.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sass": "^3.2.0",
    "gatsby-plugin-typescript": "^2.12.0",
    "gatsby-source-google-spreadsheets": "^3.0.7",
    "node-sass": "^5.0.0",
    "preact": "^10.5.12",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-table": "^6.10.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-webpack-plugin": "^2.5.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "netlify-cli": "^3.5.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "gatsby",
    "beer"
  ],
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "dev": "gatsby develop",
    "format": "prettier --write src/**/*.{ts,tsx,css} *.{json,js,md}",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "gatsby clean",
    "deploy": "netlify deploy",
    "start": "gatsby serve"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --write --ext .js,.jsx,.ts,.tsx",
      "git add"
    ],
    "*.{css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/butlerx/invbeertory"
  },
  "bugs": {
    "url": "https://github.com/butlerx/invbeertory/issues"
  }
}
